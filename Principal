package aula07;

public class Principal {

	public static void main(String[] args) {
		Robo robo1 = new Robo();
		Robo robo2 = new Robo();
		float incremento = 50;
		
		robo1.ligar();
		robo1.andar();
		robo1.andar2(incremento);
		robo1.parar();
		robo1.desligar();
		
		robo2.ligar();
		robo2.andar();
		robo2.parar();
		robo2.desligar();
	}

}
